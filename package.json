{
  "name": "xiuxian-bot",
  "version": "2.0.0",
  "description": "凡人修仙文字游戏",
  "author": "ningmengchongshui",
  "license": "GPL-2.0",
  "main": "src/main.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "alemonjs dev",
    "build": "alemonjs build",
    "open": "alemonjs start --input ./lib/src/index.js",
    "img-dev": "NODE_OPTIONS='--loader alemonjs/loader --no-warnings' tsxp dev",
    "css": "tailwindcss -i ./src/input.css -o ./public/output.css",
    "css-m": "tailwindcss -i ./src/input.css -o ./public/output.css -m",
    "css-dev": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
    "start": "pm2 startOrRestart pm2.config.cjs",
    "stop": "pm2 stop pm2.config.cjs",
    "delete": "pm2 delete pm2.config.cjs",
    "format": "prettier --write .",
    "check-format": "git diff --exit-code",
    "prepare": "husky"
  },
  "dependencies": {
    "@alemonjs/discord": "^0.0.3",
    "@alemonjs/kook": "^0.0.2",
    "@alemonjs/qq": "^0.0.3",
    "@alemonjs/qq-group-bot": "^0.0.4",
    "@alemonjs/qq-guild-bot": "^0.0.4",
    "alemonjs": "2.0.0-rc.15",
    "axios": "^1.6.5",
    "bullmq": "^4.12.7",
    "chat-space": "0.0.4",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "esbuild-ts-paths": "^1.1.3",
    "ioredis": "^5.3.2",
    "mysql2": "^3.6.2",
    "puppeteer": "22",
    "react-puppeteer": "^1.1.0",
    "sequelize": "^6.33.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/css-modules": "^1.0.5",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/qs": "^6.9.9",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.5",
    "husky": "^9.0.11",
    "pm2": "^5.3.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.3"
  }
}
