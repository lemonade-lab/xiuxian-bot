{
  "exec": "ts-node index.ts",
  "verbose": true,
  "delay": 1000,
  "restartable": "rs",
  "execMap": {
    "ts": "ts-node"
  },
  "ignore": ["node_modules"],
  "watch": ["src", "alemon.*.{js,ts,tsx,jsx}"],
  "env": {
    "NODE_ENV": "development",
    "ALEMONJS_MESSAGE": "dev"
  },
  "ext": "js,ts,tsx,jsx",
  "envDir": ""
}
