{
  "name": "jsxp",
  "version": "1.0.0-rc.0",
  "author": "ningmengchongshui",
  "description": "截图tsx组件",
  "main": "./lib/index.js",
  "type": "module",
  "dependencies": {
    "crypto": "^1.0.1",
    "koa": "^2.15.3",
    "koa-mount": "^4.0.0",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "puppeteer": "^22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.22"
  },
  "types": "lib",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./.puppeteerrc": {
      "require": "./.puppeteerrc.cjs",
      "types": "./.puppeteerrc.d.ts"
    },
    "./tsconfig.json": {
      "import": "./tsconfig.json",
      "require": "./tsconfig.json",
      "types": "./tsconfig.json"
    }
  },
  "bin": {
    "jsxp": "./bin/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=16.14.0"
  }
}
